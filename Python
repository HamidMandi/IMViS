    # Functions of Push Button State

    def pushButton_340_state(self):
        if self.pushButton_340.isChecked():
            self.textBrowser.append ("The Cell of 34 Hz is ON")
            time.sleep(0.1) 
            ser.write(b'A')
        else:
            self.textBrowser.append ("The Cell of 34 Hz is OFF")  
            time.sleep(0.1)
            ser.write(b'a')

    def pushButton_345_state(self):
        if self.pushButton_345.isChecked():
            self.textBrowser.append ("The Cell of 34.5 Hz is ON")
